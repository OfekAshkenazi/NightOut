<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".BaseActivity.BaseDrawerActivity"
    tools:showIn="@layout/app_bar_base_drawer"
    android:background="@drawable/night_club_2">

    <com.sothree.slidinguppanel.SlidingUpPanelLayout
        android:id="@+id/mapPanel_navActivity"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom"
        sothree:umanoPanelHeight="0dp"
        sothree:umanoShadowHeight="0dp"
        >
        <FrameLayout
            android:id="@+id/fragmentsContainer_navActivity"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:id="@+id/map_container_navActivity"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </RelativeLayout>
    </com.sothree.slidinguppanel.SlidingUpPanelLayout>


</RelativeLayout>